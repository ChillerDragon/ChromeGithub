{
  "name": "GitHub forks",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Extend GitHub's network tab with more detail about the forks. And sort by importance.",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "120": "images/icon120.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [
        "forks.js"
      ]
    }
  ]
}
